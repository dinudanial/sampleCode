<?php
require_once 'PHPUnit/Framework/TestCase.php';
class BuslinesControllerTest extends PHPUnit_Framework_TestCase{
    
    public function savebusline(){
          $busline = new Admin_Model_Busline();
          $busline->setlineName(1);
          $busline->setinsuranceClass("Accident");
          $busline->setenabled(1);
          try{
                 $result = $busline->saveBuslines($busline);
                 $this->assertEquals($result,true);
          }catch(Exception $e) {
                 echo "Error :".$e->getMessage();
          }


    }
}
?>
